{
  "name": "presentation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/presentation/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/presentation",
        "index": "apps/presentation/src/index.html",
        "baseHref": "/",
        "main": "apps/presentation/src/main.tsx",
        "tsConfig": "apps/presentation/tsconfig.app.json",
        "assets": [
          "apps/presentation/src/favicon.ico",
          "apps/presentation/src/assets"
        ],
        "styles": [
          "apps/presentation/src/styles.scss"
        ],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/presentation/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/presentation/src/environments/environment.ts",
              "with": "apps/presentation/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "presentation:build",
        "hmr": true,
        "host": "127.0.0.1",
        "port": 3000
      },
      "configurations": {
        "development": {
          "buildTarget": "presentation:build:development"
        },
        "production": {
          "buildTarget": "presentation:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/presentation/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/presentation/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
